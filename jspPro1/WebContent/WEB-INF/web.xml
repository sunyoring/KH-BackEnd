<?xml version="1.0" encoding="UTF-8"?>

<web-app version="3.1" id="WebApp_ID" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

<display-name>jspPro1</display-name>


<welcome-file-list>

<welcome-file>index.jsp</welcome-file>

</welcome-file-list>


<filter>

<filter-name>encodingFilter</filter-name>

<filter-class>com.kh.common.filter.EncodingFilter</filter-class>

</filter>


<filter-mapping>

<filter-name>encodingFilter</filter-name>

<url-pattern>/*</url-pattern>

</filter-mapping>


<filter>

<filter-name>encryptFilter</filter-name>

<filter-class>com.kh.common.filter.EncryptFilter</filter-class>

</filter>


<filter-mapping>

<filter-name>encryptFilter</filter-name>

<url-pattern>/insert.me</url-pattern>

<url-pattern>/login.me</url-pattern>

<url-pattern>/updatePwd.me</url-pattern>

</filter-mapping>

</web-app>